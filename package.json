{
  "name": "build-homebrew",
  "version": "0.1.0",
  "description": "Atom Build provider for Homebrew formulas",
  "repository": "https://github.com/idleberg/atom-build-homebrew",
  "license": "MIT",
  "keywords": [
    "buildprovider",
    "compile",
    "homebrew",
    "brew",
    "formula"
  ],
  "main": "lib/provider.js",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "builder": {
      "description": "Builds and audits Homebrew formulas",
      "versions": {
        "2.0.0": "provideBuilder"
      }
    }
  },
  "package-deps": [
    "build"
  ],
  "dependencies": {
    "atom-package-deps": "^4.6.2"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "coffeelint-stylish": "^0.1.2",
    "eslint": "^5.5.0",
    "eslint-config-atom-build": "^4.0.0",
    "gulp": "^4.0.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^4.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-jsonlint": "^1.2.2",
    "gulp-lesshint": "^5.0.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "atom-build",
    "rules": {
      "consistent-return": 0,
      "no-console": 0
    }
  },
  "scripts": {
    "test": "gulp lint"
  }
}
